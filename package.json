{
  "name": "api-calls-store",
  "version": "0.0.2",
  "description": "API Calls Store for React Redux",
  "scripts": {
    "prettier": "prettier --write .",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:lib": "babel src -d lib/src; cp package.json lib/package.json; cp README.md lib/README.md; cp LICENSE lib/LICENSE",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll --env=./src/utils/testing/jest-environment-jsdom.js --transformIgnorePatterns \"node_modules/(?!axios)/\""
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.23.0",
    "reactstrap": "^9.2.2",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "reselect": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "axios-mock-adapter": "^1.22.0",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.1",
    "sinon": "^17.0.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ensuro/api-calls-store.git"
  },
  "keywords": [
    "api",
    "insurance",
    "ensuro",
    "redux",
    "store"
  ],
  "author": "Ensuro Dev Team <dev@ensuro.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ensuro/api-calls-store/issues"
  }
}
